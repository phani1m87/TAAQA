    </fields>
    <fields>
        <fullName>LeadSource</fullName>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Lead_Company_Name__c</fullName>
        <externalId>false</externalId>
        <formula>Lead.Company</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Lead Company Name</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Lead_Detail_Link__c</fullName>
        <externalId>false</externalId>
        <formula>&quot;https://na25.salesforce.com/&quot;+LeadId</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Lead Detail Link</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Lead_Owner_First_Name__c</fullName>
        <externalId>false</externalId>
        <formula>Lead.Owner:User.FirstName</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Lead Owner First Name</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Load_Date__c</fullName>
        <externalId>false</externalId>
        <formula>DATEVALUE(CreatedDate)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Load Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Load_Year__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(YEAR(Load_Date__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Load Year</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>MQL_Rating__c</fullName>
        <externalId>false</externalId>
        <formula>If (ISPICKVAL(Status, &quot;Contact-Me&quot;), &quot;HOT&quot;,
IF (ISPICKVAL(Status, &quot;View-Recording&quot;), &quot;WARM&quot;,
IF (ISPICKVAL(Status, &quot;Considering&quot;), &quot;WARM&quot;, 
NULL)))</formula>
        <label>MQL Rating</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
