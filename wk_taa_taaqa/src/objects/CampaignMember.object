    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Age__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Lead.IsConverted = TRUE ,  Lead.ConvertedDate -  Load_Date__c ,
IF( TEXT( Lead.Status ) = &quot;Unqualified&quot; , NULL , TODAY() - Load_Date__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Age</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Came_From_Lead_Member__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Identifies leads that were converted from an actual lead, not associated first to an existing contact</description>
        <externalId>false</externalId>
        <label>Came From Lead Member</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CampaignId</fullName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>CampaignMemberLeadId__c</fullName>
        <description>Captures Campaign Member Lead Id</description>
        <externalId>false</externalId>
        <formula>CASESAFEID(LeadId)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>CampaignMemberLeadId</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>City</fullName>
    </fields>
    <fields>
        <fullName>Comments__c</fullName>
        <externalId>false</externalId>
