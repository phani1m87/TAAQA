<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This custom setting will hold Support Site Product Listing - used in Sales to Service Integration Project which allows case creation in Service Org.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>AccountType__c</fullName>
        <description>This field holds Account Type</description>
        <externalId>false</externalId>
        <inlineHelpText>This field holds Account Type: s = Small Firm Services, o = Other</inlineHelpText>
        <label>AccountType</label>
        <length>1</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Product_Cloud_Name__c</fullName>
        <description>Holds the Product Name to pass to Service Org.</description>
        <externalId>false</externalId>
        <inlineHelpText>Holds the Product Name to pass to Service Org.</inlineHelpText>
        <label>Product Cloud Name</label>
        <length>100</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Product_Family_Name__c</fullName>
        <description>Holds the Product Family Name.</description>
        <externalId>false</externalId>
        <inlineHelpText>Holds the Product Family Name.</inlineHelpText>
        <label>Product Family Name</label>
        <length>100</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Product_Id__c</fullName>
        <description>This holds the Product Id (can be duplicates as per seed file data)</description>
        <externalId>false</externalId>
        <inlineHelpText>This holds the Product Id (can be duplicates as per seed file data)</inlineHelpText>
        <label>Product Id</label>
        <length>10</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Product_Name__c</fullName>
        <description>This field holds Support Site Product Name</description>
        <externalId>false</externalId>
        <inlineHelpText>This field holds Support Site Product Name</inlineHelpText>
        <label>Product Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
