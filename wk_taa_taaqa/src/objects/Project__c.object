        <fullName>Required_for_Prioritized_Requests</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
ISPICKVAL(Prioritization_Status__c, &quot;Reviewed - Prioritized&quot;),
ISPICKVAL(Prioritization_Status__c, &quot;Completed&quot;)
) &amp;&amp;
OR(
ISBLANK(Start_Date__c),
ISBLANK(Current_Due_Date_Completion_Date__c) 
)</errorConditionFormula>
        <errorMessage>Please enter a Start Date and Due Date.</errorMessage>
    </validationRules>
